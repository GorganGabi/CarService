<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>

  <connectionStrings>
    <add name="CarServiceContext" connectionString="Server=DESKTOP-SV5EGCB\MSSQLSERVER01; Database=Master; Trusted_Connection=True" />
  </connectionStrings>
  <system.serviceModel>
    <services>
      <service name="CarService.ObjectWCF.ImplCarService" behaviorConfiguration="metadataSupport">
        <!---->
        <endpoint address="http://localhost:8000/PC"
        binding="basicHttpBinding"
        contract="CarService.ObjectWCF.InterfaceCarService"
        name="BasicHttpBinding_InterfaceCarService">
          <identity>
            <dns value="localhost"/>
          </identity>
        </endpoint>
        <endpoint address="mex"
       binding="mexHttpBinding"
       contract="IMetadataExchange"
       name="mexhttp"/>
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior name="metadataSupport">
          <!-- Enables the IMetadataExchange endpoint in services that -->
          <!-- use "metadataSupport" in their behaviorConfiguration -->
          <!-- attribute. -->
          <!-- In addition, the httpGetEnabled and httpGetUrl -->
          <!-- attributes publish-->
          <!-- Service metadata for retrieval by HTTP/GET at the address -->
          <!-- "http://192.168.0.102:8000/SampleService?wsdl" -->
          <serviceMetadata httpGetEnabled="true" httpGetUrl=""/>
          <!-- <serviceMetadata/>-->
          <serviceDebug includeExceptionDetailInFaults="true"/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
  
</configuration>